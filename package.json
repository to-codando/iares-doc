{
  "name": "iares-doc",
  "source": "public/index.html",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "concurrently \"pnpm dev\" \"pnpm android:dev\"",
    "android:dev": "npx cap run android --livereload --external --ssl -- --ssl-cert='./ssh/server.crt' --ssl-key='./ssh/server.key'",
    "android:build": "cd android && ./gradlew assembleDebug",
    "android:install": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "android:deploy": "npm run build && npm run build:android && npm run install:android",
    "lint": "pnpm biome check --fix --verbose",
    "dev": "pnpm run build",
    "prod": "pnpm run build && cd ./dist && npx http-server -p 4444",
    "test": "mocha ./dist/**/*.spec.js",
    "test:watch": "pnpm run test && pnpm run test:watcher",
    "test:coverage": "pnpm run coverage:build && c8 --reporter=text --reporter=lcov  mocha  ./dist/tests/**",
    "coverage:build": "node  --env-file=.env ./esbuild.config.js",
    "build": "pnpm run lint && node --env-file=.env --inspect ./esbuild.config.js ",
    "api:mock": "json-server --watch api/db.json",
    "test:watcher": "run-when-changed --watch './dist/**/*.js' --exec 'mocha  ./dist/**/*.spec.js %s'"
  },
  "aliases": {
    "@/store": ["./dist/store/index.ts"],
    "@/components/*": ["./dist/components"],
    "@/utils/*": ["./dist/utils"],
    "@/services/*": ["./dist/services"],
    "@/assets/*": ["./dist/assets"],
    "@/mock/*": ["./dist/mock"],
    "@/tests/*": ["./dist/tests"]
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "nyc": {
    "extension": [".ts"]
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/filesystem": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/toast": "^6.0.2",
    "@types/markdown-it": "^14.1.2",
    "fam-code-ui": "^0.0.10",
    "htm": "^3.1.1",
    "iares": "^1.2.4",
    "markdown-it": "^14.1.0",
    "marked": "^14.0.0",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@capacitor/app": "^6.0.1",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@jcesarmobile/ssl-skip": "^0.4.0",
    "@mdx-js/esbuild": "^3.0.1",
    "@mdx-js/mdx": "^3.0.1",
    "@tauri-apps/api": "2.0.0-beta.15",
    "@tauri-apps/cli": "2.0.0-beta.20",
    "@tauri-apps/plugin-shell": "2.0.0-beta.8",
    "@types/chai": "^4.3.17",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "^18.19.43",
    "@types/prismjs": "^1.26.4",
    "biome": "^0.3.3",
    "c8": "^7.14.0",
    "chai": "^4.5.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.5",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-plugin-d-ts-path-alias": "^4.2.0",
    "esbuild-plugin-dev-server": "^4.0.0",
    "esbuild-plugin-path-alias": "^1.0.7",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^9.3.5",
    "global-jsdom": "^8.8.0",
    "htmlhint": "^1.1.4",
    "http-server": "^14.1.1",
    "install-peerdeps": "^3.0.3",
    "jsdom": "^21.1.2",
    "json-server": "^0.17.4",
    "kill-port": "^2.0.1",
    "mocha": "^10.7.0",
    "module-alias": "^2.2.3",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "rehype-highlight": "^7.0.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-mdx-frontmatter": "^5.0.0",
    "run-when-changed": "^2.1.0",
    "shx": "^0.3.4",
    "stylint": "^2.0.0",
    "typescript": "^5.5.4",
    "ws": "^8.18.0"
  }
}
